{
  "name": "venom-proxy-server",
  "version": "1.0.0",
  "description": "Venom WhatsApp Proxy Server for Attendance System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-connection.js",
    "test:enhanced": "node test-enhanced.js",
    "fix:connection": "node scripts/fix-whatsapp-connection.js",
    "fix:getmaybemeuser": "node scripts/fix-getmaybemeuser.js",
    "start:enhanced": "node server.js",
    "start:clean": "npm run clean && npm start",
    "start:clean:enhanced": "npm run fix:getmaybemeuser && npm run start:enhanced",
    "start:tunnel": "node scripts/start-with-tunnel.js",
    "clean": "node scripts/clean-session.js",
    "setup": "node scripts/setup.js",
    "install-chrome": "node scripts/install-chrome.js",
    "restart:safe": "npm run fix:connection && npm start",
    "restart:enhanced": "npm run fix:getmaybemeuser && npm run start:enhanced"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "puppeteer": "^24.18.0",
    "qrcode-terminal": "^0.12.0",
    "venom-bot": "^4.2.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "whatsapp",
    "venom-bot",
    "proxy",
    "attendance"
  ],
  "author": "Ahmed Hosny",
  "license": "MIT"
}
